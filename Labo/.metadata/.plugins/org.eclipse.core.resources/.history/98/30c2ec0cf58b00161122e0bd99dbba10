package com.spring.henallux.model;

public class Hobby 
{
	private String id;
	private String name;
	
	public Hobby(String i, String n)
	{
		id = i;
		name = n;
	}
	
	public Hobby()
	{

	}
	
	public void setId(String i)
	{
		id = i;
	}
	
	public void setName(String n)
	{
		name = n;
	}
	
	public String getId()
	{
		return id;
	}
	
	public String getName()
	{
		return name;
	}
}
